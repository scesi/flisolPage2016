<!DOCTYPE html>
<html>
<head>
  <title>Flisol</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' =>true %>
  <%= csrf_meta_tags %></head>
<body class="fondo">
  <nav>
    <div class="nav-wrapper blue-grey">
      <img id="logo" src="/scesi_logo.png" width="100" height="60">
      <a href="#" data-activates="mobile-demo" class="button-collapse"> <i class="mdi-navigation-menu"></i>
      </a>
      <ul class="right hide-on-med-and-down">
        <li>
          <a class="white-text" href="/charlas"> <i>charlas</i>
          </a>
        </li>
        <li>
          <a class="white-text" href="/distros">
            <i>distros</i>
          </a>
        </li>
        <li>
          <a class="white-text" href="/evento">
            <i>evento</i>
          </a>
        </li>
        <% if usuario_signed_in? %>
        <li>
          <p>
            Bienvenido
            <%= current_usuario.email %></p>
          <%= link_to('Logout', destroy_usuario_session_path, :method =>:delete, class: "material-icons") %></li>
        <% else %>
        <li>
          <%= link_to('Login', new_usuario_session_path, class: 'white-text') %></li>
        <% end %></ul>

      <ul class="side-nav blue-grey" id="mobile-demo">
        <li>
          <a class="white-text" href="/charlas">charlas</a>
        </li>
        <li>
          <a class="white-text" href="distros">distros</a>
        </li>
        <li>
          <a class="white-text" href="evento">evento</a>
        </li>
      </ul>
    </div>
  </nav>

  <div class="parallax-container">
    <div class="parallax">
      <img src="/flisolHead1.png"></div>
  </div>

  <div class="container fondo">
    <%= yield %></div>

</br>
<!--
<div class="parallax-container">
<div class="parallax">
  <img src="/flisolFooter.png"></div>
</div>
-->
<%= javascript_include_tag 'application', 'data-turbolinks-track' =>true %>
<script>
  $(".button-collapse").sideNav();
  $(document).ready(function(){
      $('.parallax').parallax();
  });
</script>
<!--desarrollado por irvin373-->
</body>
</html>